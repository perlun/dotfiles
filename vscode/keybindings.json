// Place your key bindings in this file to overwrite the defaults
[
  { "key": "cmd+shift+s",  "command": "editor.action.sortLinesAscending",
                 "when": "editorTextFocus" },
  { "key": "shift+ctrl+up", "command": "editor.action.insertCursorAbove",
                          "when": "editorTextFocus" },
  { "key": "shift+ctrl+down", "command": "editor.action.insertCursorBelow",
                             "when": "editorTextFocus" },
  {
    "key": "cmd+ctrl+n",
    "command": "extension.advancedNewFile"
  },
  {
    "key": "up",
    "command": "cursorUp",
    "when": "editorTextFocus"
  },
  {
    "key": "up",
    "command": "-cursorUp",
    "when": "editorTextFocus"
  }
]
